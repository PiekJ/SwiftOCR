project 'framework/SwiftOCR.xcodeproj'

# Uncomment the next line to define a global platform for your project

target 'SwiftOCR' do
  platform :ios, '12.2'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftOCR
  pod 'GPUImage', '0.1.7'

  target 'SwiftOCRTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'SwiftOCR Mac' do
  platform :osx, '10.12'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftOCR Mac
  pod 'GPUImage', '0.1.7'

  target 'SwiftOCR MacTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
